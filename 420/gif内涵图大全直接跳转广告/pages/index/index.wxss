/**video.wxss**/

page {
    position: relative;
    background-color: #000000;
    height: 100%;
    width: 100%;
    margin: 0;
    padding: 0;
}


/*4.9*/

.scroll {
    width: 100%;
    height: 100%;
}

.wrap {
    width: 100vw;
    height: 100vh;
}

.detailVideo {
    width: 100%;
    height: 100vh;
    position: absolute;
    left: 0;
    top: 0;
}

.videoWrap {
    width: 100%;
    height: 103%;
    /* height: 100%; */
    position: absolute;
    left: 0;
    top: 0;
}


/*411*/

.littleCover {
    width: 100%;
    z-index: 1
}

.adCover {
    width: 100%;
    height: 103%;
    position: absolute;
    left: 0;
    top: 0;
}

#pointCover {
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 55;
}

#point {
    position: relative;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 55;
    background-image: url('https://raw.githubusercontent.com/rechun/Game/master/wxImages/pointnew.png');
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    opacity: 1;
    display: cover;
}

.waiting {
    text-align: center;
    line-height: 5rem;
    width: 100%;
    height: 100%;
    font-size: 30px;
    color: #fff;
    font-weight: bold;
    display: block;
}


/*413*/

#buttonNext {
    position: absolute;
    bottom: 2rem;
    width: 4rem;
    height: 4rem;
    left: 40%;
    /* z-index: 5555; */
}

.next {
    position: relative;
    width: 100%;
    height: 100%;
    margin: 0 auto;
